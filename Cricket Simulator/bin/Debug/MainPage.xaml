                <!--<StackPanel x:Name="RightPanel" Orientation="Horizontal" HorizontalAlignment="Right">
                    <Button x:Name="btnDelHis" Style="{StaticResource DeleteAppBarButtonStyle}" Tag="History"/>
                </StackPanel>-->
            </Grid>
        </AppBar>
    </Page.BottomAppBar>
